# Environment Variables for AI Digital Market
# Copy this file to .env.local and fill in your actual values

# Stripe Configuration
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here

# Next.js Configuration
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_nextauth_secret_key_here

# Database (if needed)
DATABASE_URL=your_database_url_here

# AI Agent API Keys - REQUIRED for agent functionality
OPENAI_API_KEY=sk-your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_claude_key_here
GOOGLE_API_KEY=your_google_api_key_here
HUGGINGFACE_API_KEY=your_huggingface_key_here
STABILITY_API_KEY=your_stability_ai_key_here
ELEVENLABS_API_KEY=your_elevenlabs_key_here
COHERE_API_KEY=your_cohere_key_here
REPLICATE_API_KEY=your_replicate_key_here

# AI Service Configuration
OPENAI_MODEL=gpt-4-turbo-preview
OPENAI_MAX_TOKENS=4000
OPENAI_TEMPERATURE=0.7

# Email Configuration (for newsletters, etc.)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_app_password_here

# Analytics (Optional)
GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
FACEBOOK_PIXEL_ID=your_pixel_id_here

# Domain Configuration
NEXT_PUBLIC_DOMAIN=onelastai.com
NEXT_PUBLIC_APP_URL=https://onelastai.com

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_CHAT=true
NEXT_PUBLIC_ENABLE_BLOG=true
